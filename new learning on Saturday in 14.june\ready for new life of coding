// countdown

let input = prompt("Enter a positive number:");

if (input === null || input.trim() === "" || isNaN(input) || Number(input) < 0) {
  console.log("CANCEL or invalid input");
} else {
  let count = Number(input);
  while (count >= 0) {
    console.log(count);
    count--;
  }
  console.log("Countdown finished!");
}

