let qustion ="what is the capital of iraq";
let answer="bagdad";
while(true){
    let userAnswer=prompt(qustion);
    if(userAnswer==null || userAnswer=="" ){
        alert("something is wrong please use the correct answer");
        break;
    }
    if(userAnswer==answer){
        alert("correct answer");
        console.log("everything is good inshallah");
    }
    else {
        alert("wrong answer");
        console.log("your question is wrong becarefull");
    }
};
