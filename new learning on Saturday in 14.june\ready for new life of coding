let t = prompt("Enter a number: ");
let kk = Number(t);

if (isNaN(kk) || kk <= 0) {
  console.log("Please enter a valid positive number.");
} else {
  for (let k = kk - 1; k > 0; --k) {
    console.log(`Multiplication between ${kk} and ${k} is ${kk * k}`);
  }
}
